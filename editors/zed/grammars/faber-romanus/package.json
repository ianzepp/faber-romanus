{
  "name": "tree-sitter-faber-romanus",
  "version": "0.1.0",
  "description": "Tree-sitter grammar for Faber Romanus Latin programming language",
  "main": "grammar.js",
  "types": "bindings/node",
  "scripts": {
    "build": "tree-sitter generate",
    "test": "tree-sitter test",
    "parse": "tree-sitter parse",
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip"
  },
  "keywords": [
    "tree-sitter",
    "parser",
    "latin",
    "faber-romanus"
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ],
  "author": "Ian Zepp <ian@ianzepp.com>",
  "license": "MIT",
  "devDependencies": {
    "tree-sitter-cli": "^0.22.0",
    "prebuildify": "^6.0.0"
  },
  "tree-sitter": [
    {
      "scope": "source.faber-romanus",
      "file-types": [
        "fab"
      ],
      "highlights": "queries/highlights.scm"
    }
  ],
  "dependencies": {
    "node-gyp-build": "^4.8.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.0"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  }
}
